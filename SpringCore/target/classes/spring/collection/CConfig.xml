<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springfremwork.org/schema/context"
	xmlns:p="http://www.springfremwork.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="spring.collection.User" name="U1">
		<property name="name" value="ram" />
		<property name="mobail">
			<list>
				<value>10258963</value>
				<value>25874102</value>
				<value>12365478</value>
				<value>1025836</value>
			</list>
		</property>
		<property name="city">
			<set>
				<value>jaipur</value>
				<value>kota</value>
				<value>dehli</value>
				<value>puna</value>
			</set>
		</property>
		<property name="cours">
			<map>
				<entry key="java" value="2 months" />
				<entry key="python" value="1.5 months" />
				<entry key="c++" value="1 months" />
			</map>
		</property>
	</bean>


</beans>



 